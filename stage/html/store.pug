- var pagename = 'Store > Moelza-Fashion '
<! DOCTYPE html >
html 
  head
    include pug/layouts/_head
    title #{pagename}
  body 
    include pug/pages/landing_store/_navbar
    .store.landing-store 
        .jumbotron.jumbotron-fluid
            .overlay 
                .container
                    h1.display-3 Fluid jumbo heading
                    p.lead It is a long established fact that a reader will be distracted by the readable content
                    hr.my-2
                    a.btn.btn-primary.btn-lg(href='Jumbo action link' role='button') Learm more 
    include pug/layouts/_footer
    include pug/components/loading